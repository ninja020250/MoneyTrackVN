{
  "ConnectionStrings": {
    "DefaultConnection": "${{DATABASE_URL}}"
  },
  "JwtSettings": {
    "SecretKey": "${{JWT_SECRET_KEY}}"
  },
  "LLMSettings": {
    "Providers": {
      "gemini": {
        "ApiKey": "${{GEMINI_API_KEY}}",
        "Model": "${{GEMINI_MODEL}}"
      }
    }
  },
  "EmailSettings": {
    "Username": "${{EMAIL_USERNAME}}",
    "Password": "${{EMAIL_PASSWORD}}"
  }
}