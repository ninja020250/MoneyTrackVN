{
  "JwtSettings": {
    "Issuer": "MoneyTrack",
    "Audience": "MoneyTrack Client",
    "AccessTokenExpirationMinutes": 527040
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {},
  "LLMSettings": {
    "DefaultProvider": "gemini",
    "Providers": {
      "gemini": {
        "Model": "gemini-2.5-flash"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExecuting",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing' "
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "EmailSettings": {
    "SenderName": "MoneyTrack",
    "SenderEmail": "nhatcuonghuynh.tech@gmail.com"
  },
  "ApiLimits": {
    "CreateTransactionFromMessageCommand": 15
  },
  "AllowedHosts": "*"
}
