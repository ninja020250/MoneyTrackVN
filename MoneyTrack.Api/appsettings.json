{
  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "MoneyTrack",
    "Audience": "MoneyTrack Client",
    "AccessTokenExpirationMinutes": 527040
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=3003;Database=postgres;Username=postgres;Password=pass123;TrustServerCertificate=True",
    "RenderDBConnection": ""
  },
  "LLMSettings": {
    "DefaultProvider": "gemini",
    "Providers": {
      "gemini": {
        "ApiKey": "",
        "Model": "gemini-2.5-flash"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExecuting",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing' "
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "SenderName": "MoneyTrack",
    "SenderEmail": "no-reply@moneytrack.com",
    "Username": "",
    "Password": ""
  },
  "AllowedHosts": "*"
}
